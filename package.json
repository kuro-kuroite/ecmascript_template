{
  "scripts": {
    "babel:compile": "yarn babel src/js --out-dir lib --source-maps inline",
    "babel:watch": "yarn babel src/js --watch --out-dir lib --source-maps inline",
    "eslint:fix": "yarn eslint src/js --fix",
    "eslint:fix:dry": "yarn eslint src/js --fix-dry-run",
    "lint": "yarn lint:all",
    "lint:all": "yarn lint:js",
    "lint:all:dry": "yarn lint:js:dry",
    "lint:js": "run-s prettier:fix eslint:fix",
    "lint:js:dry": "run-s prettier:fix:dry eslint:fix:dry",
    "//": "escape \"yarn lint\" for windows",
    "lint:watch": "watch \"yarn lint:all\" src/js",
    "precommit": "lint-staged",
    "prettier:fix": "yarn prettier --write src/js/**/*.js",
    "prettier:fix:dry": "yarn prettier --list-different src/js/**/*.js",
    "prettier:watch:config": "cat .gitignore .prettierignore > tmp/._prettierignore & watch \"yarn prettier --write * --ignore-path tmp/._prettierignore\" .",
    "watch": "yarn watch:all",
    "watch:all": "yarn run-p lint:watch webpack:watch",
    "webpack:watch": "yarn webpack --watch"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier --write",
      "yarn eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "parallelshell": "^3.0.2",
    "prettier": "^1.14.2",
    "watch": "^1.0.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  },
  "license": "MIT"
}
